doctype html
html(lang="en")
  head
    include base_head
  body
    include banner
    div.content
      div.scorecontainer
        each item in result
          div.score
            if (item.startTime)
              h5=item.homeName
              h5=item.visitorName
              h5=item.startTime
            else
              h5=item.homeName + "  " + item.homeScore
              h5=item.visitorName + "  " + item.visitorScore